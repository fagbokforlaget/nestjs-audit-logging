{
  "name": "@forlagshuset/nestjs-audit-logging",
  "version": "1.0.1",
  "description": "Nestjs Audit Logging Package",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fagbokforlaget/nestjs-audit-logging.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fagbokforlaget/nestjs-audit-logging/issues"
  },
  "homepage": "https://github.com/fagbokforlaget/nestjs-audit-logging#readme",
  "devDependencies": {
    "@nestjs/platform-express": "^10.2.5",
    "@nestjs/testing": "^10.2.5",
    "@types/jest": "^29.5.5",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@forlagshuset/audit-logging": "^1.0.2",
    "@nestjs/common": "^10.2.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "setupFiles": [
      "./jest-setup-file.ts"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "^.+\\.js$"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
